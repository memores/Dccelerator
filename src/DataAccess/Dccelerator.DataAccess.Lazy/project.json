{
  "version": "0.0.12-*",
  "title": "Dccelerator.DataAccess.Lazy",
  "description": "Lazy Loading and navigation properties support for Dccelerator.DataAccess ORM.",
  "authors": [ "Kirill Dubinkin" ],
  "packOptions": {
    "summary": "Lazy Loading and navigation properties support for Dccelerator.DataAccess ORM.",
    "tags": [ "ORM Lazy Loading" ],
    "projectUrl": "https://github.com/we-apon/Dccelerator",
    "licenseUrl": "https://github.com/we-apon/Dccelerator/blob/master/LICENSE"
  },



  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ],
        "xmlDoc": true
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true,
        "xmlDoc": true
      }
    }
  },


  "frameworks": {

    "net40": {
      "buildOptions": { "define": [ "NET40", "JETBRAINS_ANNOTATIONS" ] },
      "dependencies": { "PostSharp": "4.3.29" }
    },


    "net45": {
      "buildOptions": { "define": [ "NET45", "JETBRAINS_ANNOTATIONS" ] },
      "dependencies": { "PostSharp": "4.3.29" }
    },

    "netstandard1.3": {
      "buildOptions": { "define": [ "NET_STANDARD", "JETBRAINS_ANNOTATIONS" ] },
      "dependencies": { "PostSharp": "5.0.25-preview" }

    }

  },


  "dependencies": {
    "Dccelerator.DataAccess.Common": "0.0.12-*"
  }

}
