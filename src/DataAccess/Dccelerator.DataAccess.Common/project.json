{
  "version": "0.0.12-*",
  "title": "Dccelerator.DataAccess.Common",
  "description": "Contains interfaces and base implementation of Dccelerator.DataAccess ORM.",
  "authors": [ "Kirill Dubinkin" ],
  "packOptions": {
    "summary": "Base interfaces of Dccelerator.DataAccess ORM.",
    "tags": [ "ORM" ],
    "projectUrl": "https://github.com/we-apon/Dccelerator",
    "licenseUrl": "https://github.com/we-apon/Dccelerator/blob/master/LICENSE"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ],
        "xmlDoc": true
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true,
        "xmlDoc": true
      }
    }
  },
  "frameworks": {
    "net40": {
      "buildOptions": { "define": [ "NET40", "JETBRAINS_ANNOTATIONS" ] }
    },
    "net45": {
      "buildOptions": { "define": [ "NET45", "JETBRAINS_ANNOTATIONS" ] }
    },
    "netstandard1.3": {
      "buildOptions": { "define": [ "NET_STANDARD", "JETBRAINS_ANNOTATIONS" ] }
    }
  },

  "dependencies": {
    "Dccelerator.Core": "0.0.12-*"
  }

}

