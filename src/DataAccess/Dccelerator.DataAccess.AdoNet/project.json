{
  "version": "0.0.12-*",
  "title": "Dccelerator.DataAccess.AdoNet",
  "description": "Contains ADO.NET adapter classes for Dccelerator.DataAccess ORM",
  "authors": [ "Kirill Dubinkin" ],
  "packOptions": {
    "summary": "ADO.NET adapter classes for Dccelerator.DataAccess ORM",
    "tags": [ "ORM ADO.NET" ],
    "projectUrl": "https://github.com/we-apon/Dccelerator",
    "licenseUrl": "https://github.com/we-apon/Dccelerator/blob/master/LICENSE"
  },


  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ],
        "xmlDoc": true
      }
    },
    "Release": {
      "buildOptions": {
        "optimize": true,
        "xmlDoc": true
      }
    }
  },


  "frameworks": {
    "net40": {
      "buildOptions": { "define": [ "NET40", "JETBRAINS_ANNOTATIONS" ] },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0",
        "System.Data": "4.0.0.0"
      }
    },

    "net45": {
      "buildOptions": { "define": [ "NET45", "JETBRAINS_ANNOTATIONS" ] },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0",
        "System.Data": "4.0.0.0"
      }
    }

    /*
    "netstandard1.3": {
      "buildOptions": { "define": [ "NET_STANDARD", "JETBRAINS_ANNOTATIONS" ] }
    }*/

  },


  "dependencies": {
    "Dccelerator.DataAccess.Common": "0.0.12-*"
  },

}
